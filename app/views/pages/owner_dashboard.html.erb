<div class="container">
<h1 class="page-header">My Gods</h1>
<div>
  <% @gods.each do |god| %>
 <div> <p><%= cl_image_tag god.photo, height: 200, width: 300, crop: :fill, class:"rounded-image" %> </p></div>
  <div><h3> Upcoming bookings for <%= god.name %></h3></div>
 <div>  <p> <% god.bookings.each do |booking| %> </p>
    <% start_date = booking.start_date %>
    <% end_date = booking.end_date %>
    <li> <%= start_date.strftime('%A, %d %b %y') %> - <%=end_date.strftime('%A, %d %b %y') %></li></div>
    <% end %>
  </p>
  <% end %>
</div>
</div>
</div>
